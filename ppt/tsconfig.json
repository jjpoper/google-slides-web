{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@util/": [
        "src/util"
      ],
      "@api/": [
        "./src/api"
      ],
      "@model/": [
        "./src/model"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "./typings",
    "src/utils/googleAuth.js",
    "src/utils/drawText.js"
, "src/components/resize/index.js", "src/store/modules/teacher.js", "src/utils/shareScreen.js"],
  "exclude": [
    "node_modules"
  ]
}